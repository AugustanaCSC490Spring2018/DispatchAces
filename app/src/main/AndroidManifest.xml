<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kevinbarbian14.dispatchaces">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dispatcher_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dispatcher_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity
            android:name=".Google_SignIn"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".AboutPageActivity"> android:label="About"></activity>

    </application>

</manifest>